    "color",
    "tracker",
    "interaction",
    "hover",
    "hidden"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "http://bl.ocks.org/vasturiano"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vasturiano/canvas-color-tracker/issues"
  },
